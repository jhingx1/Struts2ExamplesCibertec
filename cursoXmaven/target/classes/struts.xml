<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="default" extends="struts-default" namespace="/">
        
        <!-- Profesores -->
        <action name="profesoresQry" class="web.action.ProfesoresAction">
            <result name="success">/view/profesoresQry.jsp</result>
        </action>
        
        <action name="profesoresInsForm" class="web.action.ProfesoresAction">
            <result name="success">/view/profesoresIns.jsp</result>
        </action>
        
        
        
        <!-- Cursos -->
        <action name="cursosQry" class="web.action.CursosAction">
            <result name="success">/view/cursosQry.jsp</result>
        </action>
        <action name="cursosInsForm" class="web.action.CursosAction">
            <result name="success">/view/cursosIns.jsp</result>
        </action>
        <action name="cursosIns" class="web.action.CursosAction"
                method="cursosIns">
            <result name="input">/view/cursosIns.jsp</result>
            <result name="success" type="redirect">cursosIns</result> 
            <result name="error">/view/cursosIns.jsp</result>  
        </action>
        
        
    </package>
</struts>

